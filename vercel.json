{ 
"builds":[ 
      { "src": "package.json", 
         "use": "@vercel/static-build", 
          "config": { "distDir": "dist" } 
      },
      { "src": "dist/server.js", 
        "use": "@vercel/node", 
        "config": { "includeFiles": ["dist/**"] } 
      } 
 ], 
 "routes": [ { "src": "/(.*)", "dest": "dist/server.js" } ] }